    /* Extra Small Devices, Phones */ 
  @media only screen and (min-width : 480px)  and (max-width: 660px) {
      h1 {font-size:26px}
      h2{font-size:25px}
      h3{font-size:12px}
      #aMenuHoriz { display: block; }
      #fecha{font-size:13px}
      .tituIcon{font-size:13px}      
      .text-pie{
	     font-family: "Arial", Helvetica, sans-serif;
         font-size: 14px;
         color: #ffffff;
        } 
     .imgBanners{
     	width:200px;height:30px;
     }  
    }
    
     /* Small Devices, Tablets */
    @media only screen and (min-width : 668px) {
      h1 {font-size:28px}
      h2{font-size:26px}
      h3{font-size:12px}
      #aMenuHoriz { display: none; }
      #fecha{font-size:11px}
      .tituIcon{font-size:9px}
      .text-pie{
	     font-family: "Arial", Helvetica, sans-serif;
         font-size: 14px;
         color: #ffffff;                 
          }
        } 
        
        .imgBanners{
     	width:200px;height:30px;
     }  
    }
       
      /* Small Devices, Tablets */
    @media only screen and (min-width : 768px){
      h1 {font-size:28px}
      h2{font-size:26px}
      h3{font-size:12px}
      #aMenuHoriz { display: none; } 
      #fecha{font-size:11px}
      .tituIcon{font-size:11px}
      .text-pie{
	     font-family: "Arial", Helvetica, sans-serif;
         font-size: 14px;
         color: #ffffff;                 
          }
        } 
        
        .imgBanners{
     	width:200px;height:30px;
     }  
    
   
                  
    /* Medium Devices, Desktops */
     @media only screen and (min-width : 992px) {
       h1 {font-size:32px}
       h2{font-size:28px}
       h3{font-size:25px}
       #aMenuHoriz { display: none; }
       #fecha{font-size:20px}
       .tituIcon{font-size:14px}
	   .text-pie{
	     font-family: "Arial", Helvetica, sans-serif;
	     font-size: 16px;
	     color: #ffffff;
	    }
	    .imgBanners{
	 	width:220px;height:40px;
     } 
            
    }
   
    /* Large Devices, Wide Screens */
    @media only screen and (min-width : 1200px) {
       h1 {font-size:34px}
       h2{font-size:30px}
       h3{font-size:26px}
       #fecha{font-size:24px}
       #aMenuHoriz { display: none; }
       .tituIcon{font-size:15px}
       .text-pie{
	     font-family: "Arial", Helvetica, sans-serif;
         font-size: 16px;
         color: #ffffff;
        }
        .imgBanners{
     	width:220px;height:40px;
     } 
    }
    

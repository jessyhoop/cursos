           //alert("ajax funcionando 1= "+ data);  //as a debugging message.
           
           var obj = jQuery.parseJSON(data); //datos javascript  
           
          //alert("ajax funcionando 2= "+ obj.evaluacion[0].idcaptura);                   
                
               $('#adatos').val(obj.evaluacion[0].idcaptura);
               $('#adatos').attr('disabled',true);
                          
                      },
                  error: function(errorThrown){
